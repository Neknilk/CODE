import pandas as pd

file_path = 'C:/Users/jayva/Documents/GitHub/FOE/DATA/FILTERED/combined_data_filled.csv'
df = pd.read_csv(file_path, low_memory=False)

# Count the number of rows in the original DataFrame
original_rows = len(df)

# Assuming 'flight_duration_seconds' is the column you want to filter
df = df[df['flight_duration_seconds'] <= 10000]

# Count the number of rows in the modified DataFrame
modified_rows = len(df)

# Print the amount of deleted data
deleted_rows = original_rows - modified_rows
print(f"Number of rows deleted: {deleted_rows}")

# Save the modified DataFrame back to the original file
df.to_csv(file_path, index=False)
