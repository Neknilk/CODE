import pandas as pd

# Replace 'your_file.csv' with the actual path to your CSV file
file_path = r'C:\Users\jayva\Documents\GitHub\FOE\DATA\FILTERED\combined_data_filled.csv'

# Read the CSV file into a Pandas DataFrame
df = pd.read_csv(file_path)

# Replace 'your_column_name' with the actual name of the column you're interested in
column_name = 'epoch_time'

# Filter and print rows where the values are not integers in the specified column
non_integer_rows = df[~df[column_name].astype(str).str.isdigit()]

print(f"Rows with non-integer values in the column '{column_name}':")
print(non_integer_rows)